{
    "aggregations": {
        "nested_agg": {
            "nested": {
                "path": "nested_obj"
            },
            "aggregations": {
                "my_nested_terms_agg": {
                    "terms": {
                        "field": "keyword",
                        "size": 10,
                        "shard_size": -1,
                        "min_doc_count": 1,
                        "shard_min_doc_count": 0,
                        "show_term_doc_count_error": false,
                        "order": [
                            {
                                "_count": "desc"
                            },
                            {
                                "_term": "asc"
                            }
                        ]
                    }
                }
            }
        }
    },
    "ext": {}
}